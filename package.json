{
  "name": "gjs-presets",
  "version": "0.0.1",
  "description": "presets lib for grapesjs",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src",
    "build": "npm run v:patch && webpack --env.production",
    "start": "webpack-dev-server --open --progress --colors",
    "deploy": "git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magr0s/gjs-presets.git"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "peerDependencies": {
    "grapesjs": "0.x"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.13.0",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "keywords": [
    "grapesjs",
    "plugins"
  ],
  "author": "magr0s",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magr0s/gjs-presets/issues"
  },
  "homepage": "https://github.com/magr0s/gjs-presets#readme",
  "dependencies": {
    "@amcharts/amcharts4": "^4.2.4",
    "grapesjs-blocks-basic": "^0.1.8"
  }
}
